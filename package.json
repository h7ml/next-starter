{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "description": "A production-ready Next.js starter template with React, TypeScript, Tailwind CSS, shadcn/ui, Prisma, and Docker support.",
  "author": "h7ml <h7ml@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/h7ml/next-starter.git"
  },
  "bugs": {
    "url": "https://github.com/h7ml/next-starter/issues"
  },
  "homepage": "https://github.com/h7ml/next-starter#readme",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "tailwindcss",
    "shadcn-ui",
    "prisma",
    "docker",
    "postgresql"
  ],
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.0.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prebuild": "sh scripts/generate-build-info.sh",
    "build": "next build --webpack",
    "analyze": "ANALYZE=true next build --webpack",
    "dev": "next dev",
    "lint": "pnpm lint:eslint && pnpm lint:tslint",
    "lint:eslint": "eslint . --quiet",
    "lint:eslint:fix": "eslint . --fix --quiet",
    "lint:tslint": "tslint -p tsconfig.json -c tslint.json",
    "lint:tslint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "lint:fix": "pnpm lint:eslint:fix && pnpm lint:tslint:fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --cache --log-level error .",
    "format:check": "prettier --check --cache --log-level error .",
    "lint:i18n:json": "prettier --check --log-level error \"dictionaries/**/*.json\"",
    "fix": "pnpm format && pnpm lint:fix",
    "fix:check": "pnpm fix && pnpm format:check && pnpm lint:i18n:json && pnpm lint && pnpm typecheck",
    "check": "pnpm format:check && pnpm lint:i18n:json && pnpm lint && pnpm typecheck",
    "start": "next start",
    "postinstall": "node -e \"if (process.env.DATABASE_URL) { require('child_process').execSync('prisma generate', { stdio: 'inherit' }) }\"",
    "prisma:generate": "prisma generate",
    "prisma:types": "prisma generate --generator dts",
    "prisma:push": "prisma db push && pnpm run prisma:types",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "latest",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.0.0",
    "@prisma/generator-helper": "^6.0.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@tailwindcss/postcss": "^4.1.9",
    "@tiptap/extension-placeholder": "^3.3.2",
    "@tiptap/react": "^3.3.2",
    "@tiptap/starter-kit": "^3.3.2",
    "@types/node": "^22",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/analytics": "1.3.1",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "framer-motion": "12.25.0",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.12",
    "postcss": "^8.5",
    "prisma": "^6.0.0",
    "react": "19.2.0",
    "react-day-picker": "9.8.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "shiki": "^3.21.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.9",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "vaul": "^1.1.2",
    "zod": "3.25.76"
  },
  "overrides": {
    "prisma": "^6.0.0",
    "@prisma/client": "^6.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "16.0.10",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.10",
    "prettier": "^3.3.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
